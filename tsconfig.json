{
  "compilerOptions": {
      "target": "ES6",
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "commonjs",
      "baseUrl": ".",
      "checkJs": true,
      "strict": true,
      "allowJs": true,
      "lib": [
        "ES2015",
        "DOM"
      ]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
